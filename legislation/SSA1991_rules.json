{
  "name": "Social Security Act 1991 (Demo Excerpt)",
  "version": 2,
  "lastUpdated": "2024-07-01",
  "jurisdiction": "Commonwealth of Australia",
  "sections": [
    {
      "id": "SSA1991 s.35(2)",
      "title": "Senior rate threshold",
      "when": "age >= 65",
      "then": "apply_rate: senior",
      "description": "Individuals aged 65 years or older qualify for the senior benefit rate"
    },
    {
      "id": "SSA1991 s.12(3)",
      "title": "Residency requirement",
      "when": "residency_months >= 10",
      "then": "eligibility: true",
      "description": "Minimum 10 months Australian residency required for eligibility"
    },
    {
      "id": "SSA1991 s.40",
      "title": "Income cap (prescribed amount)",
      "when": "annual_income > 85000",
      "then": "reduce_benefit: formula_40A",
      "description": "Income exceeding $85,000 triggers benefit reduction"
    }
  ],
  "rounding": "round-half-up(2)",
  "currency": "AUD"
}

